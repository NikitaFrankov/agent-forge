{
  "name": "agent-forge",
  "version": "1.4.0",
  "description": "Pluggable development pipeline with executor system - workflow commands + stack-specific executors (kotlin, rust, python, typescript, go)",
  "author": {
    "name": "NikitaFrankov"
  },
  "keywords": [
    "pipeline",
    "agents",
    "development",
    "automation",
    "pluggable",
    "executor",
    "kotlin",
    "rust",
    "python",
    "typescript",
    "go"
  ],
  "features": {
    "executors": ["kotlin", "rust", "python", "typescript", "go", "default"],
    "workflows": ["init", "feature", "fix", "analyze", "refactor"],
    "hooks": ["PreToolUse", "SessionStart"]
  }
}
